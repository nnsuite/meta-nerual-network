From 415bf03a2e9e8458d4c4cea636994314687f1835 Mon Sep 17 00:00:00 2001
From: Yongjoo Ahn <yongjoo1.ahn@samsung.com>
Date: Wed, 3 Feb 2021 17:20:24 +0900
Subject: [PATCH] [test/SSAT] Adjust tensor_rate ssat test for yocto

- Reduce videotestsrc image size 640x480 to 32x16 for yocto qemu

Signed-off-by: Yongjoo Ahn <yongjoo1.ahn@samsung.com>
---
 tests/nnstreamer_rate/runTest.sh | 10 +++++-----
 1 file changed, 5 insertions(+), 5 deletions(-)

diff --git a/tests/nnstreamer_rate/runTest.sh b/tests/nnstreamer_rate/runTest.sh
index 4e3b9047..461c9b86 100644
--- a/tests/nnstreamer_rate/runTest.sh
+++ b/tests/nnstreamer_rate/runTest.sh
@@ -22,10 +22,10 @@ testInit $1
 PATH_TO_PLUGIN="../../build"
 
 # Dump original frames, passthrough
-gstTest "--gst-plugin-path=${PATH_TO_PLUGIN} videotestsrc num-buffers=300 ! video/x-raw,width=640,height=480,framerate=30/1 ! tensor_converter ! multifilesink location=original_%1d.log" 1 0 0 $PERFORMANCE
+gstTest "--gst-plugin-path=${PATH_TO_PLUGIN} videotestsrc num-buffers=300 ! video/x-raw,width=32,height=16,framerate=30/1 ! tensor_converter ! multifilesink location=original_%1d.log" 1 0 0 $PERFORMANCE
 
 # Adjust frame rates (30 --> 15), downscaled with frame drops
-gstTest "--gst-plugin-path=${PATH_TO_PLUGIN} videotestsrc num-buffers=300 ! video/x-raw,width=640,height=480,framerate=30/1 ! tensor_converter ! tensor_rate framerate=15/1 throttle=false ! multifilesink location=result_%1d.log" 2 0 0 $PERFORMANCE
+gstTest "--gst-plugin-path=${PATH_TO_PLUGIN} videotestsrc num-buffers=300 ! video/x-raw,width=32,height=16,framerate=30/1 ! tensor_converter ! tensor_rate framerate=15/1 throttle=false ! multifilesink location=result_%1d.log" 2 0 0 $PERFORMANCE
 
 callCompareTest original_0.log result_0.log 2-1 "Compare 2-1" 1 0
 callCompareTest original_150.log result_75.log 2-2 "Compare 2-2" 1 0
@@ -34,7 +34,7 @@ callCompareTest original_200.log result_100.log 2-3 "Compare 2-3" 1 0
 rm result_*.log
 
 # Adjust frame rates (30 --> 3), downscaled with frame drops
-gstTest "--gst-plugin-path=${PATH_TO_PLUGIN} videotestsrc num-buffers=300 ! video/x-raw,width=640,height=480,framerate=30/1 ! tensor_converter ! tensor_rate framerate=3/1 throttle=false ! multifilesink location=result_%1d.log" 3 0 0 $PERFORMANCE
+gstTest "--gst-plugin-path=${PATH_TO_PLUGIN} videotestsrc num-buffers=300 ! video/x-raw,width=32,height=16,framerate=30/1 ! tensor_converter ! tensor_rate framerate=3/1 throttle=false ! multifilesink location=result_%1d.log" 3 0 0 $PERFORMANCE
 
 callCompareTest original_0.log result_0.log 3-1 "Compare 3-1" 1 0
 callCompareTest original_150.log result_15.log 3-2 "Compare 3-2" 1 0
@@ -43,7 +43,7 @@ callCompareTest original_200.log result_20.log 3-3 "Compare 3-3" 1 0
 rm result_*.log
 
 # Adjust frame rates (30 --> 45), upscaled with frame duplication
-gstTest "--gst-plugin-path=${PATH_TO_PLUGIN} videotestsrc num-buffers=300 ! video/x-raw,width=640,height=480,framerate=30/1 ! tensor_converter ! tensor_rate framerate=45/1 throttle=false ! multifilesink location=result_%1d.log" 4 0 0 $PERFORMANCE
+gstTest "--gst-plugin-path=${PATH_TO_PLUGIN} videotestsrc num-buffers=300 ! video/x-raw,width=32,height=16,framerate=30/1 ! tensor_converter ! tensor_rate framerate=45/1 throttle=false ! multifilesink location=result_%1d.log" 4 0 0 $PERFORMANCE
 
 callCompareTest original_0.log result_0.log 4-1 "Compare 4-1" 1 0
 callCompareTest original_150.log result_225.log 4-2 "Compare 4-2" 1 0
@@ -52,7 +52,7 @@ callCompareTest original_200.log result_300.log 4-3 "Compare 4-3" 1 0
 rm result_*.log
 
 # Adjust frame rates (30 --> 60), upscaled with frame duplication
-gstTest "--gst-plugin-path=${PATH_TO_PLUGIN} videotestsrc num-buffers=300 ! video/x-raw,width=640,height=480,framerate=30/1 ! tensor_converter ! tensor_rate framerate=60/1 throttle=false ! multifilesink location=result_%1d.log" 5 0 0 $PERFORMANCE
+gstTest "--gst-plugin-path=${PATH_TO_PLUGIN} videotestsrc num-buffers=300 ! video/x-raw,width=32,height=16,framerate=30/1 ! tensor_converter ! tensor_rate framerate=60/1 throttle=false ! multifilesink location=result_%1d.log" 5 0 0 $PERFORMANCE
 
 callCompareTest original_0.log result_0.log 5-1 "Compare 5-1" 1 0
 callCompareTest original_150.log result_300.log 5-2 "Compare 5-2" 1 0
-- 
2.17.1

